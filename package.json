{
  "name": "nexbox-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig .json -s ./src -o ./dist",
    "start": "ts-node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --ignore node_modules --transpile-only src/index.ts",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "migrate:run": "sequelize-cli db:migrate",
    "migrate:reset": "sequelize-cli db:migrate:undo",
    "test": "jest"
  },
  "dependencies": {
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.25.5",
    "sequelize-cli": "^6.5.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tscpaths": "^0.0.9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/node": "^16.11.10",
    "jest": "^29.3.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.5.2"
  }
}
